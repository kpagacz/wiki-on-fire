{
  "name": "giddy-banana",
  "version": "0.1.0",
  "description": "The backend service of WikiOnFire",
  "main": "app.js",
  "scripts": {
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "dev": "npm run migrate && npm run seed && npx nodemon --exec node --experimental-specifier-resolution=node app.js",
    "start": "npx nodemon --exec node --experimental-specifier-resolution=node app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "jsdoc -c jsdoc.config.json -p package.json -r README.md controllers/* middleware/* migrations/* models/* routes/* seeders/* services/* src/* ./app.js",
    "deploy-docs": "aws s3 rm s3://jsdocs-backend-wof/ --recursive && aws s3 cp ./out/giddy-banana/0.1.0/ s3://jsdocs-backend-wof/ --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kpagacz/wiki-on-fire.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kpagacz/wiki-on-fire/issues"
  },
  "homepage": "https://github.com/kpagacz/wiki-on-fire#readme",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "core-js": "^3.19.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mysql2": "^2.3.2",
    "sequelize": "^6.9.0"
  },
  "type": "module",
  "devDependencies": {
    "eslint": "^8.3.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.1.1",
    "esm": "^3.2.25",
    "prettier": "^2.5.1",
    "prettier-eslint": "^8.2.2",
    "prettier-plugin-jsdoc": "^0.3.30",
    "sequelize-cli": "^6.3.0"
  }
}
